#!/usr/bin/env node

process.title = 'wow';

var path = require('path');

console.log(path.resolve('.'));

process.argv = [
    'node',
    'grunt',
    'local',
    '-b',
    '.',
    '--gruntfile',
    path.resolve('.') + path.sep + 'Gruntfile.js'
];

console.log(process.argv);

// init Grunt task
require('grunt').cli();